@{
}

<div id="mainrv"></div>

@section Scripts {
    <script>
        $(document).ready(function (){
            $.ajax({
                url: '@Url.Action("GlobalReviewAJAX", "Review")',
                success: function (response) {
                    $("div#mainrv").html(response)
                }
            });
        });
    </script>
}
